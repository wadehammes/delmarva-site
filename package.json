		"jest-mock": "^30.0.5",
		"lodash.flatten": "^4.4.0",
		"mapbox-gl": "^3.15.0",
		"next": "^15.5.3",
		"next-intl": "^4.3.9",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-preset-env": "^10.3.1",
		"postcss-syntax": "^0.36.2",
		"react": "^19.1.1",
		"react-aria": "^3.43.2",
		"react-dom": "^19.1.1",
		"react-google-recaptcha": "^3.1.0",
		"react-hook-form": "^7.62.0",
		"react-intersection-observer": "^9.16.0",
		"react-player": "^3.3.2",
		"resend": "^6.1.0",
