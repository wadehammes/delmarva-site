{
  "name": "delmarva-site",
  "version": "0.0.0",
  "packageManager": "pnpm@10.11.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5656",
    "build": "next build && make sitemap",
    "start": "next start -p 5656",
    "export": "next export",
    "tsc:ci": "tsc --strict",
    "lint": "biome check",
    "lint:ci": "biome ci --reporter=github",
    "lint:fix": "biome check --fix",
    "test:ci": "jest --passWithNoTests",
    "types:contentful": "export $(cat .env.local | sed '/^#/d; s/[$\"]//g'); cf-content-types-generator -s $CONTENTFUL_SPACE_ID -t $CONTENTFUL_CMA_TOKEN -o src/contentful/types -X && biome format --write src/contentful/types"
  },
  "dependencies": {
    "@contentful/rich-text-plain-text-renderer": "^17.0.0",
    "@contentful/rich-text-react-renderer": "^16.0.1",
    "@contentful/rich-text-types": "^17.0.0",
    "@faker-js/faker": "^9.8.0",
    "@jest/types": "^29.6.3",
    "@next/third-parties": "^15.3.2",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.76.2",
    "classnames": "^2.5.1",
    "contentful": "^11.5.23",
    "html-react-parser": "^5.2.5",
    "jest-mock": "^29.7.0",
    "lodash.flatten": "^4.4.0",
    "next": "^15.3.2",
    "next-intl": "^4.1.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^10.1.6",
    "postcss-syntax": "^0.36.2",
    "react": "^19.1.0",
    "react-aria": "^3.40.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "schema-dts": "^1.1.5",
    "tslib": "^2.8.1",
    "usehooks-ts": "^3.1.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/lodash.flatten": "^4.4.9",
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "cf-content-types-generator": "^2.16.0",
    "csstype": "^3.1.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react-is": "^19.1.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}
