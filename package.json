{
	"name": "delmarva-site",
	"version": "0.0.0",
	"packageManager": "pnpm@10.17.0",
	"private": true,
	"scripts": {
		"dev": "next dev -p 5656",
		"build": "next build",
		"build:analyze": "ANALYZE=true next build",
		"start": "next start -p 5656",
		"export": "next export",
		"tsc:ci": "tsc --strict",
		"lint": "biome check",
		"lint:ci": "biome ci --reporter=github",
		"lint:fix": "biome check --fix",
		"lint:write": "biome check --write",
		"test:ci": "jest --passWithNoTests",
		"types:contentful": "export $(cat .env.local | sed '/^#/d; s/[$\"]//g'); cf-content-types-generator -s $CONTENTFUL_SPACE_ID -t $CONTENTFUL_CMA_TOKEN -o src/contentful/types -X && biome format --write src/contentful/types",
		"update-readme": "node scripts/update_main_readme.js",
		"setup-hooks": "bash scripts/setup-git-hooks.sh"
	},
	"dependencies": {
		"@contentful/rich-text-plain-text-renderer": "^17.1.3",
		"@contentful/rich-text-react-renderer": "^16.1.3",
		"@contentful/rich-text-types": "^17.2.2",
		"@faker-js/faker": "^9.9.0",
		"@jest/types": "^30.0.5",
		"@next/third-parties": "^15.5.3",
		"@react-email/components": "^0.5.3",
		"@react-email/render": "^1.2.3",
		"@svgr/webpack": "^8.1.0",
		"@tanstack/react-query": "^5.89.0",
		"@types/mapbox-gl": "^3.4.1",
		"clsx": "^2.1.1",
		"contentful": "^11.7.19",
		"gsap": "^3.13.0",
