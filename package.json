		"lodash.flatten": "^4.4.0",
		"mapbox-gl": "^3.16.0",
		"next": "^16.0.7",
		"next-intl": "^4.5.5",
		"postcss-flexbugs-fixes": "^5.0.2",
		"postcss-preset-env": "^10.4.0",
		"postcss-syntax": "^0.36.2",
		"react": "^19.2.0",
		"react-aria": "^3.44.0",
		"react-dom": "^19.2.0",
		"react-google-recaptcha": "^3.1.0",
		"react-hook-form": "^7.66.1",
		"react-intersection-observer": "^10.0.0",
		"react-player": "^3.4.0",
