		"@testing-library/user-event": "^14.6.1",
		"@types/jest": "^30.0.0",
		"@types/lodash.flatten": "^4.4.9",
		"@types/node": "^24.5.0",
		"@types/react": "^19.1.13",
		"@types/react-dom": "^19.1.9",
		"@types/react-google-recaptcha": "^2.1.9",
		"cf-content-types-generator": "^2.16.1",
		"csstype": "^3.1.3",
		"jest": "^30.1.3",
